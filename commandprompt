local zhkey = "bpPazkwYWtDmV729wUtuhEFC"
local Discord = "https://discord.gg/ZB2twzHKsF"

colorcodes = {"@@BLACK@@", "@@BLUE@@", "@@GREEN@@", "@@CYAN@@", "@@MAGENTA@@", "@@BROWN@@", "@@LIGHT_GRAY@@", "@@DARK_GRAY@@", "@@LIGHT_BLUE@@","@@LIGHT_GREEN@@","@@LIGHT_CYAN@@","@@LIGHT_RED@@","@@LIGHT_MAGENTA@@","@@YELLOW@@","@@WHITE@@"}

function checkfiles()
   if isfile("KeyStorage.CMDP") then
    if readfile("KeyStorage.CMDP") == zhkey then
        return(true)
    end
    end
end
function loadscript()
rconsoleclear()
rconsoleprint('@@WHITE@@')
rconsoleprint("Microsoft Windows [Version 0.1.0]\n")
rconsoleprint("(c) Microsoft Corporation. All rights reserved.\n\n")
function startscript()
rconsoleprint("C/Users/Administrator>")
local s = rconsoleinput()
if s == "cmds" then
    rconsoleprint("\nCMDS      Displays available commands\nSILENTAIM     A type of hack that allows a player to shoot another and kill them w/out looking at them\nNOCLIP     A type of cheat that allows a player to go through walls\nTITLE    Change the title of the window\nCLEAR    Clears the console\nCOLOR    Changes the color of the text inputted onto the console (AVAILABLE COLORS: https://pastebin.com/raw/iYAc5EbN)\nMESSAGEBOX     Creates a message box\nESP     Allows you to see players through walls and terrain\nTRIGGERBOT     Clicks the mouse button whenever a person walks infront of the cursor\nAIMBOT     Makes the cursor follow the target\nFLY    Allows a player to move through the air without being affected by gravity\nANTIAFK    Disables being kicked by the Anti-AFK system\nANTIKICK    Disables being kicked by local scripts\nSPEED    Changes the walkspeed of a player\nINFJUMP    Disables the cooldown for jumping\nCTRLCLICK    Teleports the player to the cursor\nPRINT    Prints a message to the console\nTIME    Prints the time to the console\nTELEPORT    Teleports the player to another player\nWALLRUN    Allows a player to walk on walls\nCROSSHAIR    Makes a GUI Crosshair\n\n")
    startscript()
elseif s == "silentaim" then
    loadstring(game:HttpGetAsync("https://raw.githubusercontent.com/Astro-beep/zlscripts/main/universalsilent"))()
    rconsoleprint("\n")
    startscript()
elseif s == "noclip" then
    loadstring(game:HttpGetAsync("https://raw.githubusercontent.com/Astro-beep/zlscripts/main/noclip"))()
    rconsoleprint("Press N to noclip\n")
    startscript()
elseif s == "title" then
    rconsoleprint("Please input the title you want the window to be.\n")
    b = rconsoleinput()
    rconsolename(b)
    rconsoleprint("\n")
    startscript()
elseif s == "clear" then
   rconsoleclear()
    rconsoleprint("\n")
    startscript()
elseif s == "color" then
    rconsoleprint("Please input the color code you want the text to be.\n")
    b = rconsoleinput()
    if table.find(colorcodes, b) then
    rconsoleprint(b)
    rconsoleprint("Successfully inputted color code.\n")
    else
    rconsoleerr("Color Code not found or incorrect.")
    end
    rconsoleprint("\n")
    startscript()
elseif s == "messagebox" then
    rconsoleprint("Please input the messaget\n")
    b = rconsoleinput()
    rconsoleprint("Input the header you want\n")
    c = rconsoleinput()
    messagebox(b, c, 0)
    rconsoleprint("\n")
    startscript()
elseif s == "esp" then
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Astro-beep/zlscripts/main/esp", true))()
    rconsoleprint("\n")
    startscript()
elseif s == "triggerbot" then
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Astro-beep/zlscripts/main/triggerbot", true))()
    rconsoleprint("\n")
    startscript()
elseif s == "aimbot" then
getgenv().Options = {
        Enabled = true,
        Triggerbot = false,
        FOV = 150,
        Smoothnes = true,
        AimPart = "Head",
    }
loadstring(game:HttpGet("https://raw.githubusercontent.com/Lucid1212/StaticWare/main/StaticWare.lua", true))()

    rconsoleprint("\n")
    startscript()
elseif s == "fly" then
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Astro-beep/zlscripts/main/fly", true))()
    rconsoleprint("Press E to fly\n")
    startscript()
elseif s == "antiafk" then
    local bb=game:service'VirtualUser'
    game:service'Players'.LocalPlayer.Idled:connect(function()
    bb:CaptureController()
    bb:ClickButton2(Vector2.new())
    end)
    rconsoleprint("\n")
    startscript()
elseif s == "antikick" then
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Astro-beep/zlscripts/main/antikick", true))()
    rconsoleprint("\n")
    startscript()
elseif s == "speed" then
    rconsoleprint("Input the speed below\n")
    b = rconsoleinput()
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = b
    rconsoleprint("\n")
    startscript()
elseif s == "infjump" then
    local InfiniteJumpEnabled = true
    game:GetService("UserInputService").JumpRequest:connect(function()
	if InfiniteJumpEnabled then
		game:GetService"Players".LocalPlayer.Character:FindFirstChildOfClass'Humanoid':ChangeState("Jumping")
	end
    end)
    rconsoleprint("\n")
    startscript()
elseif s == "ctrlclick" then
mouse = game.Players.LocalPlayer:GetMouse()
tool = Instance.new("Tool")
tool.RequiresHandle = false
tool.Name = "Click Teleport"
tool.Activated:connect(function()
local pos = mouse.Hit+Vector3.new(0,2.5,0)
pos = CFrame.new(pos.X,pos.Y,pos.Z)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = pos
end)
tool.Parent = game.Players.LocalPlayer.Backpack
rconsoleprint("\n")
startscript()
elseif s == "print" then
    rconsoleprint("Input the message\n")
    b = rconsoleinput()
    rconsoleprint(b.."\n")
    startscript()
elseif s == "time" then
    rconsoleprint(os.date().."\n")
    startscript()
elseif s == "teleport" then
    rconsoleprint("Input the name of the person you want to teleport too\n")
    b = rconsoleinput()
    if game.Players:FindFirstChild(b) then
        game.Players.LocalPlayer.Character:MoveTo(game.Players:FindFirstChild(b).Character.HumanoidRootPart.Position)
        rconsoleprint("\n")
    else
        rconsoleprint("Could not find '"..s.."'\n\n")       
    end
    startscript()
elseif s == "wallrun" then
    loadstring(game:HttpGet("https://pastebin.com/raw/JpRUfBZe", true))()
    rconsoleprint("\n")
    startscript()
elseif s == "alwaysday" then
    lighting = game:GetService('Lighting')

    delay(1, function()
	lighting.Ambient = Color3.new(1, 1, 1)
	lighting.Brightness = 1
	lighting.OutdoorAmbient = Color3.new(1, 1, 1)
	lighting.ShadowSoftness= 0
	lighting.ClockTime = 12
    end)

    rconsoleprint("Time Set Day\n\n")
    startscript()
elseif s == "crosshair" then
    loadstring(game:HttpGet("https://pastebin.com/raw/6wZGNypA", true))()
    rconsoleprint("\n")
    startscript()
elseif s == "norecoil" then
getgenv().Toggle = true
getgenv().ValueCheck = true
local FunctionCount = 0
local ValueCount = 0

local hookrecoil = function(func)
    local hookrecoil; hookrecoil = hookfunction(func, function(...)
        local args = {...}
        if getgenv().Toggle then
            return 0 or nil
        end
        return hookrecoil(unpack(args))
    end)
end

for _, func in next, getgc(true) do
    if typeof(func) == "function" and string.find(string.lower(debug.getinfo(func).name), "recoil") then
        FunctionCount = FunctionCount + 1
        hookrecoil(func)
    elseif typeof(func) == "table" then
        for i, v in next, func do
            if typeof(v) == "function" and string.find(string.lower(debug.getinfo(v).name), "recoil") then
                FunctionCount = FunctionCount + 1
                hookrecoil(v)
            elseif getgenv().ValueCheck and typeof(i) == "string" and string.find(i, "%a+") and rawget(func, i) then
                for char in string.gmatch(i, "%a+") do
                    if string.find(string.lower(char), "recoil") then
                        ValueCount = ValueCount + 1
                        if typeof(v) == "number" then
                            rawset(func, i, 0)
                        elseif typeof(v) == "string" and tonumber(v) then
                            rawset(func, i, "0")
                        elseif typeof(v) == "Vector3" then
                            rawset(func, i, Vector3.new(0,0,0))
                        elseif typeof(v) == "CFrame" then
                            rawset(func, i, CFrame.new(0,0,0))
                        end
                    end
                end
            end
        end
    end
end

print("Fetched: " .. tostring(FunctionCount) .. " Functions")
print("Fetched: " .. tostring(ValueCount) .. " Values")
rconsoleprint("Made by yesok3877\n\n")
    startscript()
else
    rconsoleprint("'"..s.."' is not recognized as an internal or external command,\noperable program or batch file\n\n")
    startscript()
end
end
startscript()
end

function whitelist()
    rconsolename("Administrator: Command Prompt")
    rconsoleclear()
    if checkfiles() == true then
    loadscript()
    else
    rconsoleprint('@@MAGENTA@@')
    setclipboard(Discord)
    rconsoleprint('Discord copied to clipboard.')
    rconsoleprint("\nPlease input the key found in our discord. (#scripts)\n")
    if rconsoleinput() == zhkey then
       wait(1)
       rconsoleprint("\nCorrect Key.. Launching in 3")
       wait(1)
       rconsoleprint("\nCorrect Key.. Launching in 2")
       wait(1)
       rconsoleprint("\nCorrect Key.. Launching in 1")
       wait(1)
       rconsoleprint("\nCommand Prompt launched.")
       writefile("KeyStorage.CMDP", zhkey)
       rconsoleclear()
       loadscript()
       return true
    else
       whitelist()
       rconsoleclear()
       return false
    end
    end
end

whitelist()
